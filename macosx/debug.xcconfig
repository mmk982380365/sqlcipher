// Linking
DEAD_CODE_STRIPPING = NO

// Code Generation
GCC_OPTIMIZATION_LEVEL = 0

// Preprocessing
GCC_PREPROCESSOR_DEFINITIONS = DEBUG=1 SQLITE_DEBUG=1 $(BASE_GCC_PREPROCESSOR_DEFINITIONS)
